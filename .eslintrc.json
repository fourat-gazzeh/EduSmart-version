{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
    "jsx": true
  }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "error",   
     "react/jsx-uses-vars": "error",
     "react/prop-types": "off"
    }
}
